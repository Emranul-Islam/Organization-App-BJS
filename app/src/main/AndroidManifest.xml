<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.muhammad_sohag.biteshwor_jobo_somaj">

    <uses-permission android:name="android.permission.CALL_PHONE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.muhammad_sohag.biteshwor_jobo_somaj.People" />
        <activity android:name="com.muhammad_sohag.biteshwor_jobo_somaj.Admin" />
        <activity
            android:name="com.muhammad_sohag.biteshwor_jobo_somaj.NoticeActivity"
            android:parentActivityName="com.muhammad_sohag.biteshwor_jobo_somaj.MainActivity" />
        <activity
            android:name="com.muhammad_sohag.biteshwor_jobo_somaj.AboutActivity"
            android:parentActivityName="com.muhammad_sohag.biteshwor_jobo_somaj.MainActivity" />
        <activity android:name="com.muhammad_sohag.biteshwor_jobo_somaj.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>